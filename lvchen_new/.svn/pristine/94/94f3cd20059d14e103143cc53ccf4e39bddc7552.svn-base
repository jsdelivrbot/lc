<ion-header>

  <ion-navbar>
    <ion-title>赠送统计</ion-title>
  </ion-navbar>

</ion-header>


<ion-content>
 <div class="titleBar line">
   <div class="businessGroup">业务组</div>
   <div class="giftNum">赠送量</div>   
 </div>

 <div class="totalBar line">
   <div class="totalTitle">合计</div>
   <div class="giftNum">{{sum || 0}}</div>
 </div>

 <div class="group line" *ngFor="let gift of groupGiftNum">
   <div class="groupName">{{gift.orgStoreName}}</div>
   <div class="giftNum">{{gift.saleQty}}</div>
 </div>

</ion-content>

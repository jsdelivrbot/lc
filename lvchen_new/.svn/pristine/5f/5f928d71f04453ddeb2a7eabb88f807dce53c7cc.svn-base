AC,OAAO,CAAC,GAAG,EAAE,CAAC;gBACtB,CAAC,CAAC,CAAC;YACL,CAAC;QACH,CAAC,EAAC,IAAI,CAAC,CAAC;IACV,CAAC;IAnDU,kBAAkB;QAJ9B,wEAAS,CAAC;YACT,QAAQ,EAAE,sBAAsB;WACG;SACpC,CAAC;6EAM+B;YACV,4GAAS;YACd,4GAAU;YACT,SAAS;OARf,kBAAkB,CAqD9B;IAAD,CAAC;AAAA;SArDY,kBAAkB,gB","file":"26.js","sourcesContent":["import { NgModule } from '@angular/core';\nimport { IonicPageModule } from 'ionic-angular';\nimport { ComplainDetailPage } from './complain-detail';\n\n@NgModule({\n  declarations: [\n    ComplainDetailPage,\n  ],\n  imports: [\n    IonicPageModule.forChild(ComplainDetailPage),\n  ],\n})\nexport class ComplainDetailPageModule {}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/main/salesAndgift/complain-detail/complain-detail.module.ts","import { Component } from '@angular/core';\nimport { IonicPage, NavController, NavParams } from 'ionic-angular';\nimport { AppService, AppGlobal } from '../../../../providers/service-public-service/service-public-service';\n\n/**\n * Generated class for the ComplainDetailPage page.\n *\n * See https://ionicframework.com/docs/components/#navigation for more info on\n * Ionic pages and navigation.\n */\n\n@IonicPage()\n@Component({\n  selector: 'page-complain-detail',\n  templateUrl: 'complain-detail.html',\n})\nexport class ComplainDetailPage {\n  data = {};\n  memo = '';\n  identity = 0; //-1业务员，1员工，2、3导购\n  constructor(\n    public navCtrl: NavController,\n     public navParams: NavParams,\n     public apps: AppService, \n     public appUrl:AppGlobal) {\n  }\n\n  ionViewDidLoad() {\n    this.apps.getItem('userInfo',val=>{\n        this.identity = val['tag'];\n    })\n    this.getData();\n  }\n\n  getData(){\n  \tthis.apps.httpPost('listJsonComplaintDtl.api',{\n      'id':this.navParams.get('id')\n    },data => {\n      if (data.code == -1) {\n        this.apps.toast(data.msg);\n      }else{\n        this.data = data.map;\n        this.memo = data.map.auditDetails;\n      }\n    },true);     \n  }\n\n  complaintRespone(action){\n  \tvar op = 'complaintRespone.api';\n  \tvar disMsg = '投诉回复成功！';\n  \tif (action == 1) {\n  \t\top = 'unBind.api';\n  \t\tdisMsg = '解除绑定成功';\n  \t}\n  \tthis.apps.httpPost(op,{\n      'complaintId':  this.data['complaintId'],\n      'integralCode':this.data['respondentIntegralCode'],\n      'memo': this.memo\n    },data => {\n      if (data.code == -1) {\n        this.apps.toast(data.msg);\n      }else{\n        this.apps.disAlert(disMsg, callback =>{\n           this.navCtrl.pop();\n        });\n      }\n    },true); \n  }\n\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/main/salesAndgift/complain-detail/complain-detail.ts"],"sourceRoot":""}
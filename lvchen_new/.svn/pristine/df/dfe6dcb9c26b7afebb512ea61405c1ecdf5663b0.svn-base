GAAG,CAAC,IAAI,CAAC,IAAI,CAAC;oBAC1B,EAAE,CAAC,CAAC,KAAI,CAAC,IAAI,IAAI,CAAC,CAAC,CAAC,CAAC;wBACnB,KAAI,CAAC,UAAU,GAAG,IAAI,CAAC;oBACzB,CAAC;oBACD,GAAG,CAAC,CAAC,IAAI,CAAC,GAAG,CAAC,EAAE,CAAC,GAAG,KAAI,CAAC,OAAO,CAAC,MAAM,EAAE,CAAC,EAAE,EAAE,CAAC;wBAC7C,KAAI,CAAC,WAAW,IAAI,KAAI,CAAC,OAAO,CAAC,CAAC,CAAC,CAAC,MAAM,CAAC;wBAC3C,KAAI,CAAC,QAAQ,IAAI,KAAI,CAAC,OAAO,CAAC,CAAC,CAAC,CAAC,aAAa,IAAI,SAAS,CAAC,CAAC,CAAC,KAAI,CAAC,OAAO,CAAC,CAAC,CAAC,CAAC,aAAa,CAAC,CAAC,CAAC,KAAI,CAAC,OAAO,CAAC,CAAC,CAAC,CAAC,aAAa,IAAI,SAAS,CAAC,CAAC,CAAC,KAAI,CAAC,OAAO,CAAC,CAAC,CAAC,CAAC,aAAa,CAAC,CAAC,CAAC,KAAI,CAAC,OAAO,CAAC,CAAC,CAAC,CAAC,aAAa,CAAC;oBAC3M,CAAC;gBACH,CAAC;YACH,CAAC;QACH,CAAC,EAAE,IAAI,CAAC,CAAC;IACX,CAAC;IA/DU,2BAA2B;QAJvC,wEAAS,CAAC;YACT,QAAQ,EAAE,gCAAgC;WACG;SAC9C,CAAC;6EAqB+B;YACX,4GAAS;YACR,UAAU;OAtBpB,2BAA2B,CAgEvC;IAAD,CAAC;AAAA;SAhEY,2BAA2B,iB","file":"61.js","sourcesContent":["import { NgModule } from '@angular/core';\nimport { IonicPageModule } from 'ionic-angular';\nimport { SalesPerformanceConsignPage } from './sales-performance-consign';\n\n@NgModule({\n  declarations: [\n    SalesPerformanceConsignPage,\n  ],\n  imports: [\n    IonicPageModule.forChild(SalesPerformanceConsignPage),\n  ],\n})\nexport class SalesPerformanceConsignPageModule {}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/grantMarket/sales-performance-consign/sales-performance-consign.module.ts","import { Component } from '@angular/core';\nimport { IonicPage, NavController, NavParams } from 'ionic-angular';\nimport { AppService } from '../../../providers/service-public-service/service-public-service';\n\n/**\n * Generated class for the SalesPerformanceConsignPage page.\n *\n * See https://ionicframework.com/docs/components/#navigation for more info on\n * Ionic pages and navigation.\n */\n\n@IonicPage()\n@Component({\n  selector: 'page-sales-performance-consign',\n  templateUrl: 'sales-performance-consign.html',\n})\nexport class SalesPerformanceConsignPage {\n  public Title: string = \"寄存额\";\n  public storeName: string = \"业务组\";\n  // 业绩类型\n  public tag: number = 1;\n  // 季度\n  public dateType: number = 1;\n  public titleArr: object = {\n    3: \"寄存销售额\",\n    7: \"寄存领取额\",\n    8: \"寄存剩余额\"\n  };\n  public tableTitle: string = \"业务组\";\n  public listMap: Array<any> = [];\n  public tail: number = 0;\n  public amountCount: number = 0;\n  public numCount: number = 0;\n\n  identity = 0; //-1业务员，1员工，2、3导购\n  constructor(\n    public navCtrl: NavController,\n    public navParams: NavParams,\n    public appService: AppService) {\n    this.appService.getItem('userInfo', val => {\n      this.identity = val['tag'];\n    })\n  }\n\n  ionViewDidLoad() {\n    this.tag = this.navParams.data.type;\n    this.Title = this.titleArr[this.tag];\n    // 获取业务组名称\n    this.appService.getItem(\"curStore\", (obj) => {\n      this.storeName = obj.storeName;\n    });\n    // 获取数据\n    this.getData();\n  }\n\n  getData() {\n    this.appService.httpPost(\"findSecoundSalesAnalysis.api\", {\n      tag: this.tag,\n      dateType: this.dateType\n    }, (res) => {\n      console.log(res);\n\n      if (res.code == 1) {\n        this.listMap = res.data.listMap;\n        if (this.identity == 2 || this.identity == 3) {\n          this.tableTitle = \"时间\";\n        }\n        else {\n          this.tail = res.data.tail;\n          if (this.tail == 1) {\n            this.tableTitle = \"门店\";\n          }\n          for (let i = 0; i < this.listMap.length; i++) {\n            this.amountCount += this.listMap[i].amount;\n            this.numCount += this.listMap[i].surplusNumSum != undefined ? this.listMap[i].surplusNumSum : this.listMap[i].receiveNumSum != undefined ? this.listMap[i].receiveNumSum : this.listMap[i].depositNumSum;\n          }\n        }\n      }\n    }, true);\n  }\n}\n\n\n\n// WEBPACK FOOTER //\n// ./src/pages/grantMarket/sales-performance-consign/sales-performance-consign.ts"],"sourceRoot":""}
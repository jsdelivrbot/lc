page-codeview {
    background: #fff!important;
    .popover-ios .item-ios {
        /* padding-left: 16px; */
        padding-left: 0 !important;
    }
   
    .item-ios {
        padding-left: 0 !important;
        border-radius: 0;
        position: relative;
        font-size: 1.5rem;
        color: #000;
        background-color: #fff;
        -webkit-transition: background-color 200ms linear;
        transition: background-color 200ms linear;
    }
    .label-ios {
        margin: 11px 8px 11px 10px;
    }
    ion-label {
        margin: 0;
        display: block;
        overflow: hidden;
        -webkit-box-flex: 1;
        -webkit-flex: 1;
        -ms-flex: 1;
        flex: 1;
        font-size: inherit;
        text-overflow: ellipsis;
        white-space: nowrap;
    }
    .popover-content{
      border: 1px solid black; 
    }
   
    
}

page-sales-return {
  .content{
    background:#f5f4f3 !important;
}
    .pageMenuSlides {
        height: 48px !important;
        .swipercontainer {
          width: 100%;
          height: 48px;
        }
        background: #fff;
        .swiper-slide {
            width: 33%;
          color: #999;
          font-size: 16px;
          float: left;
        } 
        .bottomLine {
          color: #217938;
        }
        .bottomLine:after {
          content: '';
          position: absolute;
          left: 20%;
          top: auto;
          bottom: 0;
          right: auto;
          height: 2px;
          width: 60%;
          background-color: #217938;
        }
        
      }
      .list-item-left {
        float: left;
        h2 {
          color: #333;
        }
        h4 {
          color: #999;
        }
      }
      .list-item-right {
        float: right;
        height: 3rem;
        line-height: 3rem;
        color: #999;
        .num-text {
          color: #00783a;
        }
      }
      .list-ios{
        margin:0 !important;
       
      }
      .list{
        margin-bottom: -1px !important;
      }
}
